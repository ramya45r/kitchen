

                    <!-- DataTales Example -->
                    <div class="card shadow mb-4 ">
                        <div class="card-header py-3 d-flex justify-content-between">
                            <h6 class="m-0 font-weight-bold text-primary">COUPON MANAGEMENT</h6>
                        <a href="/admin/addcoupon"><button type="button" class="btn btn-primary">Add coupon</button></a>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                        
                                            <th>Coupon Name</th>
                                            <th>Coupon Code</th>
                                            <th>limit</th>
                                            <th>Discount</th>

                                            <th>Expiration Time</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    
                                    <tbody>
                                        {{#each AllCoupons}}
                                        <tr>
                                            <td>{{this.couponName}}</td>
                                            <th>{{this.couponCode}}</th>
                                            <td>{{this.limit}}</td>
                                            <td>{{this.discount}}</td>
                                            <td>{{this.expirationTime}}</td>
                                            <td>
                                                 {{!-- <a href="/admin/Editproduct/{{this._id}}" class="btn btn-primary ">Edit</a> --}}
                                                 {{!-- <button onclick="removeProductforcart('{{this._id}}')">  <i class="bi bi-trash"></i> </button> --}}
                                                 <button onclick="deletecoupon('{{this._id}}')">  <i class="bi bi-trash "></i> </button>
                                                 {{!-- <a href="/admin/deleteproduct/{{this._id}}" class="btn btn-danger delete-button">Delete</a> --}}
                                            </td>

                                        </tr>
                                        {{/each}}
                                       
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
<script>
    // delete popup
    function deletecoupon(proId) {
					Swal.fire({
						title: 'Are you sure?',
						text: "You won't be able to revert this!",
						icon: 'warning',
						showCancelButton: true,
						confirmButtonColor: '#3085d6',
						cancelButtonColor: '#d33',
						confirmButtonText: 'Yes, delete it!'
					}).then((result) => {
						if (result.isConfirmed) {
							Swal.fire(
								'Deleted!',
								'Your file has been deleted.',
								'success'
							).then(() => {
								deleted()
							})
						}
					})
					function deleted() {
						$.ajax({

							url: '/admin/deletecoupon/'+proId,
							
							method: 'get',
							success: (response) => {
								if (response.coupondeleted) {
									// alert('product deleted')
									location.reload()
								}
							}
						})
					}
				}
                </script>
		
  
   
               