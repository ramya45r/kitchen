  <script>
   $('#checkoutForm').submit((e) => {
      e.preventDefault()
      $.ajax({
        url: '/placeOrder',
        method: 'post',
        data: $('#checkoutForm').serialize(),
        success: (response) => {
          if (response.codSuccess) {
            location.href = '/viewOrderDetails'
          } else {
            razorpayPayment(response)
          }
        }
      })
    });



